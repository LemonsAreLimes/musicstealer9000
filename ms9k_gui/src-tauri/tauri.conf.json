{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "ms9k_gui",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "protocol": {
        "all": false,
        "asset": true,
        "assetScope": ["*"]
      },
      "all": true,
      "shell": {
        "all": true,
        "open": true
      },
      "fs":{
        "all": true,
        "copyFile": false,
        "createDir": false,
        "exists": false,
        "readDir": false,
        "readFile": false,
        "removeDir": false,
        "removeFile": false,
        "renameFile": false,
        "scope": [],
        "writeFile": false
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev.123",
      "targets": "all"
    },
    "security": {
      "csp": ""
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ms9k_gui",
        "width": 800,
        "height": 600
      }
    ]
  }
}
